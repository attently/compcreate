{
	"name": "@attently/compcreate",
	"version": "1.2.2",
	"description": "A command line tool to create React components",
	"preferGlobal": true,
	"bin": {
		"compcreate": "index.js"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"deploy": "git checkout master && git merge dev && git push origin master && yarn publish && git tag \"v$(./index.js -v)\" && git checkout dev && git merge master"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/attently/compcreate.git"
	},
